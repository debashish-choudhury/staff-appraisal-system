<h5>Annexure 3</h5><br>
<div class="container">
    <h6>3.4 Consultancy Assignment</h6>
    <hr>
    {{#if result}}
    {{#each result}}
    <div class="row">
        <div class="input-field col s6 md5">
            <input id="rolesAndResponsilbilty" type="text" name="rolesAndResponsilbilty"
                value="{{rolesAndResponsilbilty}}" disabled class="validate" required>
            <label for="rolesAndResponsibility">Roles &nbsp; <a href="#" class="tooltipped"
                    data-tooltip="Roles and Responsibilities"><i class="fa fa-info-circle"></i></a></label>
        </div>

        <div class="input-field col s6 md5">
            <input id="typeOfWorkorDomain" type="text" name="typeOfWorkorDomain" value="{{typeOfWorkorDomain}}" disabled
                class="validate" required>
            <label for="typeOfWorkorDomain">Type Of Work/Domain</label>
        </div>
    </div>

    <div class="row">
        <div class="input-field col s6 md5">
            <input id="organization" type="text" name="organization" value="{{organization}}" disabled class="validate"
                required>
            <label for="organization">Organization</label>
        </div>
        <div class="input-field col s6 md5">
            <input id="duration" type="number" name="duration" value="{{duration}}" disabled class="validate" required>
            <label for="duration">Duration</label>
        </div>
    </div>

    <div class="row">

        <div class="input-field col s6">
            <input id="numberofVisits" type="number" name="numberofVisits" value="{{numberofVisits}}" disabled
                class="validate" required>
            <label for="numberofVisits">Number Of Visits</label>
        </div>
    </div>
    <a href="/annexure-3/consultancyAssignment/edit/{{id}}"
        class="btn waves-effect waves-light red center mb-2">Edit</a>
    <a href="/users/faculty/facultyOverview" class="btn waves-effect waves-light purple center mb-2">Back to
        overview</a><br><br>
    <form method="post" action="/annexure-3/consultancyAssignment/delete/{{id}}?_method=DELETE">
        <input type="hidden" name="_method" value="DELETE">
        <input type="submit" class="btn waves-effect waves-light center red white-text" value="Delete">
    </form>
</div>
</div>
</div>
{{else}}
{{/each}}
{{else}}
{{#if editResult}}
<form action="/annexure-3/consultancyAssignment/{{editResult.id}}?_method=PUT" method="post" class="col s12">
    <input type="hidden" name="_method" value="PUT">
    <div class="row">
        <div class="input-field col s6 md5">
            <input id="rolesAndResponsilbilty" type="text" name="rolesAndResponsilbilty"
                value="{{editResult.rolesAndResponsilbilty}}" class="validate" required>
            <label for="rolesAndResponsibility">Roles &nbsp; <a href="#" class="tooltipped"
                    data-tooltip="Roles and Responsibilities"><i class="fa fa-info-circle"></i></a></label>
        </div>

        <div class="input-field col s6 md5">
            <input id="typeOfWorkorDomain" type="text" name="typeOfWorkorDomain"
                value="{{editResult.typeOfWorkorDomain}}" class="validate" required>
            <label for="typeOfWorkorDomain">Type Of Work/Domain</label>
        </div>
    </div>

    <div class="row">
        <div class="input-field col s6 md5">
            <input id="organization" type="text" name="organization" value="{{editResult.organization}}"
                class="validate" required>
            <label for="organization">Organization</label>
        </div>
        <div class="input-field col s6 md5">
            <input id="duration" type="number" name="duration" value="{{editResult.duration}}" class="validate"
                required>
            <label for="duration">Duration</label>
        </div>
    </div>

    <div class="row">

        <div class="input-field col s6">
            <input id="numberofVisits" type="number" name="numberofVisits" value="{{editResult.numberofVisits}}"
                class="validate" required>
            <label for="numberofVisits">Number Of Visits</label>
        </div>
    </div>

    <div>
        <p>
            <label>
                <input id="indeterminate-checkbox" type="checkbox" />
                <span>I acknowledge that above filled information is verified and justified by me.</span>
            </label>
        </p>
    </div>
    <button class="btn waves-effect waves-light" type="submit" id="submit_btn" disabled>Submit
    </button>
</form>
</div>
</div>
</div>
{{else}}
<form action="/annexure-3/consultancyAssignment" method="post" class="col s12">
    <div class="row">

        <div class="input-field col s6 md5">
            <input id="rolesAndResponsilbilty" type="text" name="rolesAndResponsilbilty" class="validate" required>
            <label for="rolesAndResponsibility">Roles &nbsp; <a href="#" class="tooltipped"
                    data-tooltip="Roles and Responsibilities"><i class="fa fa-info-circle"></i></a></label>
        </div>

        <div class="input-field col s6 md5">
            <input id="typeOfWorkorDomain" type="text" name="typeOfWorkorDomain" class="validate" required>
            <label for="typeOfWorkorDomain">Type Of Work/Domain</label>
        </div>
    </div>

    <div class="row">
        <div class="input-field col s6 md5">
            <input id="organization" type="text" name="organization" class="validate" required>
            <label for="organization">Organization</label>
        </div>
        <div class="input-field col s6 md5">
            <input id="duration" type="number" name="duration" class="validate" required>
            <label for="duration">Duration</label>
        </div>
    </div>

    <div class="row">

        <div class="input-field col s6">
            <input id="numberofVisits" type="number" name="numberofVisits" class="validate" required>
            <label for="numberofVisits">Number Of Visits</label>
        </div>
    </div>

    <div>
        <p>
            <label>
                <input id="indeterminate-checkbox" type="checkbox" />
                <span>I acknowledge that above filled information is verified and justified by me.</span>
            </label>
        </p>
    </div>

    <a href="/annexure-3/externalProjectsOrCompetition" class="waves-effect waves-light btn center red">Not
        Applicable</a>
    <button class="btn waves-effect waves-light" type="submit" id="submit_btn" disabled>Submit
    </button>
</form>
</div>

{{/if}}
{{/if}}